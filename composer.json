{
    "name": "rooberthh/php-http-tools",
    "description": "Basic Enums for HTTP methods and status codes.",
    "type": "library",
    "version": "0.1.1",
    "keywords": [
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roberth Evaldsson",
            "email": "evaldsson.roberth@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/rooberthh/php-http-tools/issues"
    },
    "funding": [],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.13.7",
        "phpstan/phpstan": "^1.10.50",
        "roave/security-advisories": "dev-latest"
    },
    "provide": {
        "psr-discovery/http-client-implementations": "^1.0"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "Rooberthh\\HttpTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rooberthh\\HttpTools\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "classmap-authoritative": true
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ]
    },
    "scripts-descriptions": {
        "pint": "Run Laravel Pint on the codebase."
    }
}
